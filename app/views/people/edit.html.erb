<h1>Edit Page</h1>
<%= form_for @person do |f| %>
  <%= f.label :full_name %>
  <%= f.text_field :full_name%>

  <%= f.label :age %>
  <%= f.text_field :age%>

  <%= f.label :gender %>
  <%= f.text_field :gender %>

  <%= f.label :hair_color %>
  <%= f.text_field :hair_color %>

  <%= f.label :eye_color %>
  <%= f.text_field :eye_color %>

  <%= f.submit %>
<% end %>